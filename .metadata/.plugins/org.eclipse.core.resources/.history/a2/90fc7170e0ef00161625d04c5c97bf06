package br.com.fiap.bean;

import java.lang.annotation.Annotation;
import java.lang.reflect.Field;

import br.com.fiap.annotation.Coluna;
import br.com.fiap.annotation.Tabela;


public class Avestruz extends Animal{

	@Override
	public void andar(){
		System.out.println("Andando diferente...");
	}
	
	public String retornaSQL(Animal animal){
		
		
		Tabela anota = animal.getClass().getAnnotation(Tabela.class);
		
		
			return "SELECT * FROM " + anota;
		
		
		
		
	}
	
}
